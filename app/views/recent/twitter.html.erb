<%= flash[:notice] %>
<br />
<% @status_messages.each do |status_message| %>
  <div>
    <div>
      <a target="_blank" href="http://www.twitter.com/<%= status_message.screen_name %>">
        <img src="<%= status_message.profile_image %>" style="border-width: 0px;">
      </a>
    </div>
    <div>
      <%= status_message.text %>
    </div>
  </div>
  <br />
  <br />
<% end %>
